{
    "presets": [
        "@babel/preset-react",

        ["@babel/preset-env"],
        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true,
                "allowNamespaces": true,
                "allowDeclareFields": true
            }
        ]
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ]
    ]
}
